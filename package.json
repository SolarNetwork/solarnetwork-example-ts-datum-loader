  "version": "1.1.1",
  "homepage": "https://go.solarnetwork.net/dev/example/typescript-datum-loader/",
  "author": {
    "name": "Matt Magoffin",
    "email": "matt@solarnetwork.net"
  },
  "license": "Apache-2.0",
  "type": "module",
  "engines": {
    "node": ">=18.18"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "ava": {
    "files": [
      "src/test/ts/**/*"
    ],
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      },
